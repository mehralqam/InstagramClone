
<h2>Follow requests</h2>
<ul>
  <% @requests.each do |request|%>
  <%= request.user_name%>
   <%= link_to 'Accept',followships_path(:user_id => request.user_id, :follower_id => request.follow_user_id), :method => :post,class: "btn btn-secondary" , id: "public_follow", remote: true %>
  <%end%>
</ul>
