<% if user_signed_in? %>
    <div>
    <%= link_to posts_path do%>
    <%= image_tag current_user.avatar , width: "100px" , class: "rounded-circle"%>
    <h3><%= current_user.user_name %></h3>
    <% end %>
    </div>
    <div class=" d-flex justify-content-center mb-5 ">
      <%= form_tag(users_path, method: :get, remote: true, "data-type": :"script") do %>
        <%= text_field_tag(:search, params[:search]) %>
        <%= button_tag("Search", type: "submit",class:"btn btn-outline mb1 blue") %>
      <%end%>
      </div>
      <div>
        <%= render partial: "follower", locals: { users: @users } %>
      </div>
      <div class = "search_result ">
        <%= render partial: "search", locals: { users: @searched_users } %>
      </div>
      <div>
        <%= render partial: "feed"%>
      </div>
<%end%>
